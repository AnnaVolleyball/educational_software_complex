       <attribute name="title">
        <string>Оптическая схема осветительного, наблюдательного каналов, канала фиксации и канала контроля</string>
       </attribute>
       <layout class="QGridLayout" name="gridLayout_6">
        <item row="1" column="0">
         <layout class="QHBoxLayout" name="horizontalLayout">
          <item>
